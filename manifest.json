{
  "manifest_version": 2,

  "name": "Temp_tab",
  "description": "This extension allows you to store tabs temporarily without wasting memory",
  "version": "1.0",

  "background": {
    "scripts": ["background.js"]
  },

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "permissions": [
    "https://secure.flickr.com/",
    "http://*/*",
    "https://*/*",
    "tabs",
    "contextMenus",
    "tabs"
  ],
  "icons": {
    "16": "icon.png"
  }
}
